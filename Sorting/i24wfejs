#include <stdio.h>
#include <stdlib.h>

typedef struct node {
    char c;
    struct node* next;
} Node;

typedef struct LinkedList {
    Node* start;
} LinkedList;

int deleteNum(LinkedList *list) {
    int res = 0;

    Node *now = list->start;
    Node *startNum = NULL;
    int deleteNum = 0;

    if (list->start->c != ' ') {
        while (now) {
            if (now->next->c == ' ')
                break;

            if (now->c > now->next->c) {
                deleteNum = 1;
                break;
            }
        }

        if (deleteNum) {
            while (list->start) {
                if (list->start->c == ' ')
                    break;
                Node*
                delete = list->start;
                list->start = list->start->next;
                free(delete);
                res++;
            }
            now = list->start;
            deleteNum = 0;
        }
    }

    while (now) {
        if (now->c != ' ') {
            if (now->next != NULL && now->next->c != ' ') {
                if (now->c > now->next->c) {
                    deleteNum = 1;
                }
            }
        } else {
            if (now->next != NULL) {
                if (now->next->c != ' ' && startNum == NULL)
                    startNum = now;
                else
                    startNum = NULL;
            } else
                startNum = NULL;
        }

        if (deleteNum == 1) {
            deleteNum = 0;
            while (startNum->next) {
                if (startNum->next->c == ' ')
                    break;
                Node*
                delete = startNum->next;
                startNum->next = delete->next;
                free(delete);
                res++;
            }
            now = startNum;
            startNum = NULL;
        } else
            now = now->next;
    }
    return res;
}